    	</Grid>
    	<Grid x:Name="ProfileLoader" Margin="34,122,26,181" Opacity="0">
    		<Image Margin="-315,-244,-265,-259" Source="/Assets/Background.png" Stretch="Fill"/>

    		<TextBlock HorizontalAlignment="Left" Margin="0,233,0,0" TextWrapping="Wrap" Text="loading your profile" VerticalAlignment="Top" FontSize="40" Width="420" TextAlignment="Center" Foreground="White" Height="59" FontFamily="/Hangout.Client;component/Fonts/Fonts.zip#Signika"/>
    		<ProgressBar HorizontalAlignment="Left" Height="15" VerticalAlignment="Top" Width="420" Margin="0,285,0,0" IsIndeterminate="True" Background="#19FFFFFF" Foreground="White"/>
    	</Grid>
    	<ScrollViewer x:Name="scrollViewer" Margin="0,79,8,74" d:LayoutOverrides="Width" Height="647">
    		<StackPanel Width="472">
    			<Canvas Tap="ProfilePicCanvas_Tap" x:Name="ProfilePicCanvas" Height="213" Margin="24,12,10,0"  toolkit:TiltEffect.IsTiltEnabled="True"  HorizontalAlignment="Center" VerticalAlignment="Center" Width="222">
    				<Image Tap="ProfilePicCanvas_Tap" x:Name="ProfileImage" Height="200" Width="200" HorizontalAlignment="Center" VerticalAlignment="Center" Canvas.Left="10" Canvas.Top="8" Source="YellowRect.png">
    					<Image.Clip>
							<EllipseGeometry Center="100,100" RadiusX="100" RadiusY="100" />
						</Image.Clip>
